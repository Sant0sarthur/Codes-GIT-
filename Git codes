(git  Bash)   //  

(git init) // Inicializa ou Reinicia a utilização do projeto

(git status) // Informa o status do versionamento do projeto

(git add "exemplo.txt") // Adiciona o arquivo em pendencia ao versionamento
(git add .) // Adiciona todos os arquivos em pendência ao versionamento

(git commit -m "explicacao da alteração") // Commit com uma mensagem explicando o que foi alterado no projeto

(git config --global user.email "email@gmail.com") // email do github
(git config --global user.name "Nome Do Usuário") // nome no github

(git add origin https...) // setando onde vai ser armazenado no github
(git push --set-upstream origin master) // definindo um branch pro armazenamento

(git push) // Enviando tudo que recebeu commit pro github

(git rm meu_arquivo.txt) // Remove o arquivo

(git log) // Histórico
